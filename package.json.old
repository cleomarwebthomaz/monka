        "vue-currency-filter": "^3.4.3",
        "vue-select": "^3.4.0",
        "vue-select2": "^0.2.6",
        "vuelidate": "^0.7.4"
    },
    "devDependencies": {
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.23.7",
        "sass-loader": "^8.0.0",
        "vue-template-compiler": "^2.6.10"
    },
    "autoload": {
        "App": "./app"
    }
